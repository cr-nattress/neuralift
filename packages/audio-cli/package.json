{
  "name": "@neuralift/audio-cli",
  "version": "0.1.0",
  "description": "CLI tool for generating and managing audio files for Neuralift training application",
  "type": "module",
  "bin": {
    "neuralift-audio": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "neuralift",
    "audio",
    "cli",
    "tts",
    "text-to-speech",
    "openai",
    "google-cloud"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google-cloud/storage": "^7.18.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "glob": "^10.5.0",
    "inquirer": "^13.1.0",
    "openai": "^6.15.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.0.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
